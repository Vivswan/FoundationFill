{
  "name": "foundation-fill",
  "version": "1.0.0",
  "description": "A browser extension to fill text fields with predefined system prompt templates",
  "scripts": {
    "prebuild": "node scripts/generate-icons.js",
    "build": "bun scripts/bun.config.js",
    "clean-build": "node scripts/clean-rebuild.js",
    "dev": "bun scripts/bun.config.js --watch",
    "dev-clean-build": "node scripts/clean-rebuild.js --dev",
    "lint": "npx eslint .",
    "typecheck": "tsc --noEmit",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "bun": "latest",
    "eslint": "^8.51.0",
    "sharp": "^0.34.1",
    "typescript": "^5.2.2"
  }
}
